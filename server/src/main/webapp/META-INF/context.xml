<?xml version="1.0" encoding="UTF-8"?>
<Context path="/ag-push">
  <Resource name="UnifiedPushDS" auth="Container" type="javax.sql.DataSource"
      maxActive="100" maxIdle="30" maxWait="10000"
      username="unifiedpush" password="unifiedpush" driverClassName="com.mysql.jdbc.Driver"
      url="jdbc:mysql://localhost:3306/unifiedpush"/>

  <ResourceLink name="BeanManager"
  	global="GlobalBeanManager"
    type="javax.enterprise.inject.spi.BeanManager"
    factory="org.jboss.weld.resources.ManagerObjectFactory"/>

</Context>
